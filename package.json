{
  "name": "zera-js-sdk",
  "version": "1.0.0",
  "description": "ZERA Network JavaScript SDK",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "node test-runner.js",
    "test:watch": "node test-runner.js --watch",
    "test:coverage": "node test-runner.js --coverage",
    "test:verbose": "node test-runner.js --verbose",
    "test:unit": "node test-runner.js --type=unit",
    "test:integration": "node test-runner.js --type=integration",
    "test:clean": "node test-runner.js --clean",
    "update:test-scripts": "node scripts/update-test-scripts.js",
    "build:proto": "cd proto && npm run build",
    "build:proto:js": "cd proto && npm run build:js",
    "build:proto:ts": "cd proto && npm run build:ts",
    "build:proto:grpc": "cd proto && npm run build:grpc",
    "proto:install": "cd proto && npm install",
    "proto:clean": "cd proto && npm run clean",
    "proto:example": "cd proto && node example.js",
    "test:api": "node test-runner.js --module=api",
    "test:blockchain": "node test-runner.js --module=blockchain",
    "test:payments": "node test-runner.js --module=payments",
    "test:proto": "node test-runner.js --module=proto",
    "test:smart-contracts": "node test-runner.js --module=smart-contracts",
    "test:test-utils": "node test-runner.js --module=test-utils",
    "test:transfer": "node test-runner.js --module=transfer",
    "test:wallet-creation": "node test-runner.js --module=wallet-creation"
  },
  "keywords": [
    "protobuf",
    "grpc",
    "zera",
    "blockchain",
    "wallet",
    "cryptography"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@noble/ed25519": "^2.0.0",
    "@noble/hashes": "^1.3.0",
    "bip32": "^4.0.0",
    "bip39": "^3.1.0"
  },
  "devDependencies": {
    "chalk": "^5.3.0",
    "glob": "^10.3.10",
    "minimist": "^1.2.8"
  }
}